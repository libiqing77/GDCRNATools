  pages = {D92--D97},
  file = {/Users/han/Zotero/storage/2MQ8JZXL/Li et al. - 2014 - starBase v2.0 decoding miRNA-ceRNA, miRNA-ncRNA a.pdf},
  eprinttype = {pmid},
  eprint = {24297251},
  pmcid = {PMC3964941},
}
