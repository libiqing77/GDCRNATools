  date = {2010-09},
  pages = {331--336},
  file = {RICH et al_2010_A PRACTICAL GUIDE TO UNDERSTANDING KAPLAN-MEIER CURVES.pdf:/Users/han/Library/Application Support/Zotero/Profiles/n80u1dxx.default/zotero/storage/DF6TFF55/RICH et al_2010_A PRACTICAL GUIDE TO UNDERSTANDING KAPLAN-MEIER CURVES.pdf:application/pdf},
